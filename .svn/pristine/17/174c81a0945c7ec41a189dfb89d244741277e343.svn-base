<ui:composition 
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:p="http://primefaces.org/ui"
    xmlns:component="http://java.sun.com/jsf/composite/components/toolbar"
    template="/WEB-INF/templates/template.xhtml">

    <ui:define name="title">
        <h:outputText value="#{msg['menu.userManagement.role']}"></h:outputText>
    </ui:define>
    <ui:define name="toolbar">
        <component:toolbar onSave="save" onCancel="pretty:add_payment_types" savePermissionKey="COMPANY_ADD"/>

    </ui:define>
    <ui:define name="content">

        <h:form prependId="false" id="object">            
            <h:messages globalOnly="false" />
            <p:panel style="border: none;">
                <f:facet name="header">
                    <h:outputText value="Setting &#187; Payment Types &#187; Add"/>                    
                </f:facet>
            </p:panel>
            <h:panelGrid columns="2" id="payment_list" columnClasses="columnLabelStyle,columnBlankLabelStyle">

                <h:outputLabel for="ment_type_description" value=" Payment Type Description: *" />
                <h:inputText id="ment_type_description" required="true" size="15" value="#{companyAddController.company.code}" />
                <p:message for="ment_type_description" />
                <p:tooltip for="ment_type_description" value="Is alfa-numeric and Must not contain any special characters except [-] and [ _ ]" showEffect="fade" hideEffect="fade"/>


                <h:outputLabel for="Enable" value="Enable: *" />
            
               <h:selectOneMenu value="#{null}">
   	<f:selectItem itemValue="Enabled" itemLabel="Enalbed" />
   	<f:selectItem itemValue="Disabled" itemLabel="Disabled" />
</h:selectOneMenu>
            </h:panelGrid>
            <p:panel style="border: none;">
                <f:facet name="header">
                    <h:outputText value=" "/>
                </f:facet>
            </p:panel>


        </h:form>

    </ui:define>
</ui:composition>
